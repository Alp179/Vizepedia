{
  "redirects": [
    {
      "source": "https://vizepedia.com/:path*",  
      "destination": "https://www.vizepedia.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "crons": [
    {
      "path": "/api/generate-sitemap",
      "schedule": "0 3 * * *"
    }
  ]
}